{
    "objective_function": "traveltime",
    "path_variables": [
      "fuel",
      "traveltime"
    ],
    "vector_names": ["uC","vC"],
    "time_unit": "days",
    "adjust_waypoints": true,
    "zero_currents": false,
    "fixed_speed": false,
    "waypoint_splitting": false,
    "smoothing_max_iterations": 2000,
    "smoothing_blocked_sic": 10.0,
    "smoothing_merge_separation": 1e-3,
    "smoothing_converged_separation": 1e-3
}
  
